        - '#Function mysql_(\w+) not found#'
        - '#Constant GETID3_HELPERAPPSDIR not found#'
        - '#Undefined variable: \$GETID3_ERRORARRAY#'
        - '#Constant GETID3_ASF_(\w+) not found#'
